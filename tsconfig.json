{
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "build",
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "incremental": true,
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "skipLibCheck": true,
    "typeRoots": ["./node_modules/@types/"],
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx"
  }
}
